<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="SQLServer" defaultModelType="flat">

        <!-- 生成的POJO实现java.io.Serializable接口 -->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />

        <!--注释-->
        <commentGenerator>
            <!-- 将数据库中表的字段描述信息添加到注释 -->
            <property name="addRemarkComments" value="true"/>
            <!-- 注释里不添加日期 -->
            <property name="suppressDate" value="true"/>
        </commentGenerator>





        <jdbcConnection
                driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver"
                connectionURL="jdbc:sqlserver://192.168.1.252:1433;databaseName=invdbf"
                userId="sa"
                password="1qaz@wsx"/>
        <javaModelGenerator targetPackage="com.smartflow.yshyerp.crc.entity"
                            targetProject="src/main/java">
        </javaModelGenerator>

        <!-- 生成mapper xml文件，并放到resources下的mapping文件夹下 -->
        <sqlMapGenerator targetPackage="mapping"  targetProject="src/main/resources">
        </sqlMapGenerator>

        <!-- 生成mapper xml对应dao接口，放到com.songguoliang.springboot.mapper包下-->
        <javaClientGenerator targetPackage="com.smartflow.yshyerp.crc.mappper"
                             targetProject="src/main/java" type="XMLMAPPER">
        </javaClientGenerator>

        <table tableName="crc">
        </table>

    </context>
</generatorConfiguration>

